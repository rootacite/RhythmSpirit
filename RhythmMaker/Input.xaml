<Window x:Class="RhythmMaker.Input"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RhythmMaker"
        mc:Ignorable="d"
        Title="Input" Height="135" Width="261">
    <Grid>
        <TextBox x:Name="BOX" HorizontalAlignment="Left" Margin="23,22,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="204" Height="31"/>
        <Button Content="OK" HorizontalAlignment="Left" Margin="41,70,0,0" VerticalAlignment="Top" Width="49" Click="Button_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="160,70,0,0" VerticalAlignment="Top" Width="48" Click="Button_Click_1"/>

    </Grid>
</Window>
